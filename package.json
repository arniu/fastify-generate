{
  "name": "fastify-generate",
  "description": "Generate fastify applications fastly",
  "preferGlobal": true,
  "version": "0.5.0",
  "bin": {
    "fastify": "bin/cli.js"
  },
  "author": "Arniu Tseng",
  "repository": "arniu/fastify-generate",
  "license": "MIT",
  "keywords": [
    "fastify",
    "generator",
    "generate",
    "plugin",
    "app",
    "api"
  ],
  "scripts": {
    "lint": "standard --fix",
    "prelint": "prettier --write './**/*.{js,md}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.{md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "standard": {
    "env": {
      "jest": true,
      "node": true
    }
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "decamelize": "^4.0.0",
    "generify": "^4.0.0",
    "yargs-parser": "^18.0.0"
  },
  "devDependencies": {
    "fastify": "^2.2.0",
    "fastify-autoload": "^1.0.0",
    "fastify-plugin": "^1.4.0",
    "husky": "^4.0.2",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.3",
    "prettier": "^1.16.0",
    "standard": "^14.0.0"
  },
  "engines": {
    "node": ">= 6"
  }
}
