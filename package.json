{
  "name": "fastify-generate",
  "description": "One command to generate a fastify application or plugin",
  "preferGlobal": true,
  "version": "0.1.1",
  "main": "bin/cli.js",
  "bin": {
    "fastify": "bin/cli.js"
  },
  "files": [
    "bin/",
    "lib/",
    "templates/",
    "LICENSE"
  ],
  "author": "Arniu Tseng",
  "repository": "arniu/fastify-generate",
  "license": "MIT",
  "keywords": [
    "fastify",
    "generator",
    "generate",
    "plugin",
    "app",
    "api"
  ],
  "scripts": {
    "lint": "standard --fix",
    "prelint": "prettier --write './**/*.{js,md}'",
    "test": "tap {test,templates}/**/*.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.{md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "decamelize": "^2.0.0",
    "generify": "^3.0.2",
    "yargs-parser": "^11.1.1"
  },
  "devDependencies": {
    "fastify": "^1.13.2",
    "fastify-autoload": "^0.6.0",
    "fastify-plugin": "^1.4.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.0",
    "standard": "^12.0.0",
    "tap": "^12.1.1"
  },
  "engines": {
    "node": ">= 6"
  }
}
